# Storage backend for Google Maps scraper: postgres, cassandra, sqlite or csv
MAPS_STORAGE=cassandra

# Cassandra settings
CASSANDRA_CONTACT_POINTS=localhost
CASSANDRA_PORT=9042
CASSANDRA_KEYSPACE=maps
CASSANDRA_LOCAL_DATA_CENTER=datacenter1

# Postgres connection string
POSTGRES_DSN=dbname=maps user=postgres host=localhost password=postgres

# SQLite database path
SQLITE_PATH=maps.db

# CSV output path
CSV_PATH=businesses.csv
